<Window x:Class="WpfLekar.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfLekar"
        mc:Ignorable="d"
        Title="MainWindow" Height="740" Width="1280" MinHeight="740" MinWidth="1280"
        WindowStartupLocation="CenterScreen" 
        >

    



    <Grid>
        
            <Frame x:Name="frame" Margin="0,0,-0.4,0.4"  NavigationUIVisibility="Visible" Grid.ColumnSpan="2" Grid.RowSpan="2">
            <Frame.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="White" Offset="3"/>
                    <GradientStop Color="#FF24DDF7" Offset="-2"/>
                    <GradientStop Color="#FF64CCF1"/>
                    <GradientStop Color="White" Offset="0.986"/>
                </LinearGradientBrush>
            </Frame.Background>
        </Frame>

    </Grid>
   
</Window>
